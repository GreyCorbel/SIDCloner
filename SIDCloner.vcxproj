    <PostBuildEvent>
      <Command>rem This requires PlatyPS module
pwsh -c "&amp; {New-ExternalHelp -Path '$(ProjectDir)Help' -OutputPath '$(ProjectDir)Help' -Force}"

rem copy files to output
copy /y "$(TargetDir)$(TargetFileName)" "$(SolutionDir)Module\SidCloner\"
copy /y "$(TargetDir)$(TargetName).xml" "$(SolutionDir)Module\SidCloner\"
copy /y "$(ProjectDir)SidCloner.psd1" "$(SolutionDir)Module\SidCloner\"
copy /y "$(ProjectDir)SidCloner.formats.ps1xml" "$(SolutionDir)Module\SidCloner\"
copy /y "$(ProjectDir)Help\SidCloner.dll-Help.xml" "$(SolutionDir)Module\SidCloner\en-us"

rem sign files
if exist C:\TFS\DigiCertUtil.exe (
        C:\TFS\DigiCertUtil.exe sign /noinput "$(SolutionDir)Module\SidCloner\$(TargetFileName)"
        C:\TFS\DigiCertUtil.exe sign /noinput ""$(SolutionDir)Module\SidCloner\AdmPwd.PS.format.ps1xml"
)</Command>
    </PostBuildEvent>
